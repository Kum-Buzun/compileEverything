[core]
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
[remote "origin"]
	fetch = +refs/heads/*:refs/remotes/origin/*
	url = https://github.com/sitaramc/gitolite
[branch "master"]
	remote = origin
	merge = refs/heads/master
[filter "shebang"]
	smudge = shebang_replace %f
	clean = shebang_restore %f
[branch "g2"]
	remote = origin
	merge = refs/heads/g2
