precond=if [[ ! -e /usr/include/asm ]] ; then echo "you need to install gcc-multilib first for gcc to compile" ; precond_failed ; fi
pre=mkdir -p ${H}/src/@@NAMEVER@@/objdir ; cd objdir
configcmd=../configure --prefix=${HULA}/@@NAMEVER@@ --enable-shared --disable-multilib --enable-languages=c,c++ --with-as=@@PATH_AS@@ --with-ld=@@PATH_LD@@
