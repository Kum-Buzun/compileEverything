+ . ./scripts/conf.sh bdb yes
+ MONMOD=nomod
+ [ xyes = xyes -o xyes = xmod ]
+ MON=monitor
+ [ yes = mod ]
+ [ xyes = xyes -a xno != xno ]
+ SASL=nosasl
+ SASL_MECH=
+ sed -e s/@BACKEND@/bdb/ -e s/^#bdb#// -e /^#~/s/^#[^#]*~bdb~[^#]*#/#omit: / -e s/^#~[^#]*~#// -e s/@RELAY@// -e s/^#relay-#// -e s/^#yes#// -e s/^#ldapno#// -e s/^#metano#// -e s/^#relayyes#// -e s/^#sqlno#// -e s/^#rdbmsno#// -e s/^#accesslogno#// -e s/^#ddsno#// -e s/^#dynlistno#// -e s/^#memberofno#// -e s/^#pcacheno#// -e s/^#ppolicyno#// -e s/^#refintno#// -e s/^#retcodeno#// -e s/^#rwmno#// -e s/^#syncprovyes#// -e s/^#translucentno#// -e s/^#uniqueno#// -e s/^#valsortno#// -e s/^#monitor#// -e s/^#nomod#// -e s/^#nosasl#// -e s/^#acino#// -e s;@URI1@;ldap://localhost:9011/; -e s;@URI2@;ldap://localhost:9012/; -e s;@URI3@;ldap://localhost:9013/; -e s;@URI4@;ldap://localhost:9014/; -e s;@URI5@;ldap://localhost:9015/; -e s;@URI6@;ldap://localhost:9016/; -e s;@PORT1@;9011; -e s;@PORT2@;9012; -e s;@PORT3@;9013; -e s;@PORT4@;9014; -e s;@PORT5@;9015; -e s;@PORT6@;9016; -e s/@SASL_MECH@// -e s;@TESTDIR@;/home/auser/compileEverything/.cpl/src/openldap-2.4.24/tests/testrun; -e s;@DATADIR@;./testdata; -e s;@SCHEMADIR@;./schema;
