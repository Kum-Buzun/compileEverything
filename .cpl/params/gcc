precond=mkdir -p ${H}/.cpl/src/@@NAMEVER@@/objdir ; anos=$(uname -r) ; if [[ "${anos#*Ubuntu}" != "${anos}" && ! -e /usr/include/asm ]] ; then echo "you need to install gcc-multilib first for gcc to compile" ; precond_failed ; fi
cdpath=${H}/.cpl/src/@@NAMEVER@@/objdir
configcmd=../configure --prefix=${HULA}/@@NAMEVER@@ --enable-shared --disable-multilib --enable-languages=c,c++ --with-as=@@PATH_AS@@ --with-ld=@@PATH_LD@@
